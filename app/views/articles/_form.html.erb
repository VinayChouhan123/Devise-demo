<%= form_with model: article do |form| %>
  
  <div>
    <%= form.label :image %><br>
    <%= form.file_field :image %>
  </div>

  <div>
    <%= form.label :title %><br>
    <%= form.text_field :title %>
  </div>

  <div>
    <%= form.label :body %><br>
    <%= form.text_area :body %><br>
  </div>

  <div>
    <%= form.label :status %><br>
    <%= form.text_field :status %><br>
  </div>

  <div>
    <%= form.submit %>
  </div>

<% end %>